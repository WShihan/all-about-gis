{
  "manifest_version": 3,
  "name": "issuer",
  "version": "0.1",
  "author": "Wangshihan",
  "description": "A simple Chrome extension for creating github issue",
  "permissions": ["activeTab", "storage", "contextMenus", "webRequest"],
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
